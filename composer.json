{
    "name": "workup/resource-navigation-tab",
    "description": "Organize your long pile of tables and relationships into structured pages.",
    "keywords": [
        "laravel",
        "nova",
        "tabs",
        "card",
        "field"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "workup/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Workup\\ResourceNavigationTab\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workup\\ResourceNavigationTab\\ResourceNavigationTabServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
